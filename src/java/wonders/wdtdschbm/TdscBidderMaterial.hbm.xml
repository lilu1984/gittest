<?xml version="1.0"?> 

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.wonders.tdsc.bo.TdscBidderMaterial" 
    table="TDSC_BIDDER_MATERIAL"
>

    <id
        name="bidderMaterialId"
        type="com.wonders.esframework.common.type.AppString"
        column="BIDDER_MATERIAL_ID"
    >
        <generator class="uuid.hex" />
    </id>

    <property
        name="bidderId"
        type="com.wonders.esframework.common.type.AppString"
        column="BIDDER_ID"
        length="64"
    />
    <property
        name="bidderPersonId"
        type="com.wonders.esframework.common.type.AppString"
        column="BIDDER_PERSON_ID"
        length="64"
    />
    <property
        name="materialCode"
        type="com.wonders.esframework.common.type.AppString"
        column="MATERIAL_CODE"
        length="4"
    />
    <property
        name="materialBh"
        type="com.wonders.esframework.common.type.AppString"
        column="MATERIAL_BH"
        length="100"
    />
    <property
        name="materialType"
        type="com.wonders.esframework.common.type.AppString"
        column="MATERIAL_TYPE"
        length="10"
    />
    <property
        name="materialCount"
        type="java.lang.Integer"
        column="MATERIAL_COUNT"
        length="2"
    />
    <property
        name="memo"
        type="com.wonders.esframework.common.type.AppString"
        column="MEMO"
        length="100"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
