<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.wonders.tdsc.kyq.bo.MiningTranApp" 
    table="JY_MINING_TRAN_APP"
>
    <id
        name="tranAppId"
        type="com.wonders.esframework.common.type.AppString"
        column="TRAN_APP_ID"
    >
        <generator class="uuid.hex" />
    </id>

    <property
        name="noticeId"
        type="com.wonders.esframework.common.type.AppString"
        column="NOTICE_ID"
        length="64"
     />
    <property
        name="noticeNo"
        type="com.wonders.esframework.common.type.AppString"
