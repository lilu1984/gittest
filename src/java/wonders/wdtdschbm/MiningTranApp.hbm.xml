    />
    <property
        name="resultCert"
        type="com.wonders.esframework.common.type.AppString"
        column="RESULT_CERT"
        length="100"
    />
    <property
        name="resultDate"
        type="java.sql.Timestamp"
        column="RESULT_DATE"
        length="100"
    />
    <property
        name="resultPrice"
        type="java.math.BigDecimal"
        column="RESULT_PRICE"
        length="20"
    />
    <property
        name="resultBidderId"
        type="com.wonders.esframework.common.type.AppString"
        column="RESULT_BIDDER_ID"
        length="64"
    />
     <property
        name="tranStatus"
        type="com.wonders.esframework.common.type.AppString"
        column="TRAN_STATUS"
        length="2"
    />
    <property
        name="tradeStatus"
        type="com.wonders.esframework.common.type.AppString"
        column="TRADE_STATUS"
        length="2"
    />
    <property
        name="isPurposeBlock"
        type="com.wonders.esframework.common.type.AppString"
        column="IS_PURPOSE_BLOCK"
        length="2"
    />
    <property
        name="spotAddPriceRange"
        type="java.math.BigDecimal"
        column="SPOT_ADD_PRICE_RANGE"
        length="20"
    />
