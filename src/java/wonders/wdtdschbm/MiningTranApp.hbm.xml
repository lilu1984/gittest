        type="com.wonders.esframework.common.type.AppString"
        column="SELL_YEAR_END"
        length="10"
    />
    <property
        name="marginAmount"
        type="java.math.BigDecimal"
        column="MARGIN_AMOUNT"
        length="20"
    />
    <property
        name="xuHao"
        type="java.lang.Integer"
        column="XU_HAO"
        length="10"
    />
    <property
        name="transResult"
        type="com.wonders.esframework.common.type.AppString"
        column="TRANS_RESULT"
        length="10"
