        column="MINING_AREA"
        length="20"
    />
    <property
        name="resources"
        type="java.math.BigDecimal"
        column="RESOURCES"
        length="20"
    />
    <property
        name="resourcesDw"
        type="com.wonders.esframework.common.type.AppString"
        column="RESOURCES_DW"
        length="10"
