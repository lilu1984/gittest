<?xml version="1.0"?> 

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="com.wonders.tdsc.bo.TdscBlockContractInfo"
    table="TDSC_BLOCK_CONTRACK_INFO"
>

    <id
        name="contractInfoId"
        type="com.wonders.esframework.common.type.AppString"
        column="CONTRACK_INFO_ID"
    >
        <generator class="uuid.hex" />
    </id>
    <property
        name="contractSignDate"
        type="java.sql.Timestamp"
        column="CONTRACK_SIGN_DATE"
        length="20"
    />
    <property
        name="electricNum"
        type="com.wonders.esframework.common.type.AppString"
        column="ELECTRIC_NUM"
        length="200"
    />
    <property
        name="contractNum"
        type="com.wonders.esframework.common.type.AppString"
        column="CONTRACK_NUM"
        length="200"
    />
    <property
        name="acceptPerson"
        type="com.wonders.esframework.common.type.AppString"
        column="ACCEPT_PERSON"
        length="200"
    />
    <property
        name="blockReviewDate"
        type="java.sql.Timestamp"
        column="BLOCK_REVIEW_DATE"
        length="20"
    />
    <property
        name="tradePaymentDemand"
        type="com.wonders.esframework.common.type.AppString"
        column="TRADE_PAYMENT_DEMAND"
        length="2000"
    />
    <property
        name="projectStartDate"
        type="java.sql.Timestamp"
        column="PROJECT_START_DATE"
        length="20"
    />
    <property
        name="projectEndDate"
        type="java.sql.Timestamp"
        column="PROJECT_END_DATE"
        length="20"
    />    
    <property
        name="contractFileUrl"
        type="com.wonders.esframework.common.type.AppString"
        column="CONTRACK_FILE_URL"
        length="200"
    />
    <property
        name="contractFileName"
        type="com.wonders.esframework.common.type.AppString"
        column="CONTRACK_FILE_NAME"
        length="200"
    />  
    <property
        name="tradeNum"
        type="com.wonders.esframework.common.type.AppString"
        column="TRADE_NUM"
        length="200"
    />     
    <property
        name="zongDiNum"
        type="com.wonders.esframework.common.type.AppString"
        column="ZONGDI_NUM"
        length="200"
    />     
    <property
        name="actionDate"
        type="java.sql.Timestamp"
        column="ACTION_DATE"
        length="20"
    />   
    <property
        name="actionUser"
        type="com.wonders.esframework.common.type.AppString"
        column="ACTION_USER"
        length="100"
    />  
    <property
        name="ifValidity"
        type="com.wonders.esframework.common.type.AppString"
        column="IF_VALIDITY"
        length="1"
    />
    <property
        name="blockId"
        type="com.wonders.esframework.common.type.AppString"
        column="BLOCK_ID"
        length="64"
    />

</class>
</hibernate-mapping>